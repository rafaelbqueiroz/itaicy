{
  "buildCommand": "cd ItaicyEcoLodge && npm install && npm run build",
  "outputDirectory": "ItaicyEcoLodge/dist",
  "installCommand": "cd ItaicyEcoLodge && npm install",
  "devCommand": "cd ItaicyEcoLodge && npm run dev",
  "framework": null,
  "functions": {
    "ItaicyEcoLodge/server/index.ts": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/ItaicyEcoLodge/server/index.ts"
    },
    {
      "source": "/(.*)",
      "destination": "/ItaicyEcoLodge/client/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
